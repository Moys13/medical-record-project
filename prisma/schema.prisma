generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model pasien {
  id           Int       @id @default(autoincrement())
  norm         String    @unique @db.VarChar(15)
  nik          String    @unique
  wna          Boolean   @default(false)
  nama         String    @db.VarChar(50)
  jeniskelamin String    @db.VarChar(10)
  tgllahir     String?
  tmptlahir    String    @db.VarChar(50)
  agama        String    @db.VarChar(10)
  alamat       String    @db.VarChar(50)
  provinsi     String    @db.VarChar(30)
  rt           String    @db.VarChar(3)
  rw           String    @db.VarChar(3)
  kecamatan    String    @db.VarChar(20)
  kelurahan    String    @db.VarChar(20)
  pekerjaan    String    @db.VarChar(20)
  pernikahan   String    @db.VarChar(15)
  pendidikan   String    @db.VarChar(10)
  goldarah     String    @db.VarChar(2)
  noasuransi   String    @db.VarChar(15)
  ibukandung   String    @db.VarChar(20)
  notlp        String    @db.VarChar(12)
  tglmasuk     DateTime  @default(now())
  tgledit      DateTime?
}
